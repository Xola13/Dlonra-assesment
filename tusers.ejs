<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List</title>
</head>
<body>
 
    <h1>List</h1>

    <table border="1" width="100%">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Surname</th>
            <th>Email</th>
        </tr>

        <% if(tusers.length > 0){ %>
            <% for(var i = 0;i<tusers.length;i++){ %>

                <tr>
                    <td><%- tusers[i].user_id %></td>
                    <td><%- tusers[i].name %></td>
                    <td><%- tusers[i].surname %></td>
                    <td><%- tusers[i].email %></td>
                </tr>
             <% } %>
             
             <% }else{ %>

                <h1>Data not found</h1>

                <% } %>

    </table>
 

</body>
</html>